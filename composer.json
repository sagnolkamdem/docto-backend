{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "api-ecosystem-for-laravel/dingo-api": "^4.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "bensampo/laravel-enum": "^5.3",
        "doctrine/dbal": "^3.5",
        "doctrine/lexer": "1.2.3",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.5",
        "jenssegers/agent": "^2.6",
        "laravel/envoy": "*",
        "laravel/framework": "^9.19",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "3.8.0",
        "lorisleiva/laravel-actions": "^2.4",
        "nwidart/laravel-modules": "^9.0",
        "profburial/wkhtmltopdf-binaries-osx": "^0.12.1",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-permission": "^5.5",
        "stevebauman/location": "^6.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.21",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "barryvdh/laravel-debugbar": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "setup": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan migrate",
            "php artisan module:seed Core",
            "php artisan module:seed Authorization",
            "php artisan module:seed Speciality",
            "php artisan module:seed User",
            "php artisan tabiblib:user:admin"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
